<head prefix="og: http://ogp.me/ns#">
  <meta charset="utf-8">
  <title>
    {% if page.title %}{{ page.title }} | {% endif %}{{ config.title }}</title>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <meta name="author" content="{{ config.author }}">
  <meta name="description" content="{{ config.description }}">
  {# <meta name="keywords" content="{{ (page.tags | join(',')) if page.tags }}"> #}
  <link rel="canonical" href="{{ url }}">
  {{ feed_tag('atom.xml') }}

  <link rel="apple-touch-icon" sizes="57x57" href="{{ url_for('icon/cloudchat-apple-touch-icon-152.png') }}">
  <link rel="apple-touch-icon" sizes="114x114" href="{{ url_for('icon/cloudchat-apple-touch-icon-152.png') }}">
  <link rel="apple-touch-icon" sizes="120x120" href="{{ url_for('icon/cloudchat-apple-touch-icon-152.png') }}">
  <link rel="apple-touch-icon" sizes="144x144" href="{{ url_for('icon/cloudchat-apple-touch-icon-152.png') }}">
  <link rel="apple-touch-icon" sizes="152x152" href="{{ url_for('icon/cloudchat-apple-touch-icon-152.png') }}">
  <link rel="apple-touch-icon" sizes="60x60" href="{{ url_for('icon/cloudchat-apple-touch-icon-152.png') }}">
  <link rel="apple-touch-icon" sizes="72x72" href="{{ url_for('icon/cloudchat-apple-touch-icon-152.png') }}">
  <link rel="apple-touch-icon" sizes="76x76" href="{{ url_for('icon/cloudchat-apple-touch-icon-152.png') }}">
  <link rel="icon" type="image/png" href="{{ url_for('icon/cloudchat-favicon-196.png') }}" sizes="196x196">
  <link rel="icon" type="image/png" href="{{ url_for('icon/cloudchat-favicon-196.png') }}" sizes="160x160">
  <link rel="icon" type="image/png" href="{{ url_for('icon/cloudchat-favicon-196.png') }}" sizes="96x96">
  <link rel="icon" type="image/png" href="{{ url_for('icon/cloudchat-favicon-196.png') }}" sizes="16x16">
  <link rel="icon" type="image/png" href="{{ url_for('icon/cloudchat-favicon-196.png') }}" sizes="32x32">

  <link rel="stylesheet" href="https://use.typekit.net/gzo7pgh.css">

  {{ css('css/main') }}
  <style>
    :root {
      ::-webkit-scrollbar {
        height: 10px;
        width: 10px;
      }
      ::-webkit-scrollbar-track {
        background: #efefef;
        border-radius: 6px;
      }
      ::-webkit-scrollbar-thumb {
        background: #d5d5d5;
        border-radius: 6px;
      }
      ::-webkit-scrollbar-thumb:hover {
        background: #c4c4c4;
      }
  </style>
</head>